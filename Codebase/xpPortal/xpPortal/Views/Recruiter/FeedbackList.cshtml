@using xpPortal.Models
@model FeedbackModel
@{
    Layout = "~/Views/Shared/RecruiterLayout.cshtml";
    ViewBag.Title = "FeedbackList";
}


<head>
    <script type="text/javascript" src="jquery-1.8.3.js"></script>
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
</head>


<div class="row">
    <div class="col-sm-12">
        <!--notification start-->
        <section class="panel">
            <header class="panel-heading header-class">
                Feedback
                <span class="tools pull-right">
                    <a href="javascript:;" class="fa fa-chevron-down"></a>

                </span>
            </header>
            <div class="panel-body">
                <form id="submitFull" class="form-horizontal">
                    <section class="panel">
                        <div class="panel-body profile-nav">
                            <table class="table table-hover tm-membr" id="example">
                                <thead>
                                    <tr>
                                        <th style="padding-left:2%">
                                            Email
                                        </th>
                                        <th style="padding-left:2%">
                                            Feedback
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>

                                    @foreach (System.Data.DataRow row in Model.FeedbackList)
                                    {

                                    <tr>
                                       
                                        @foreach (var cell in row.ItemArray)
                                            {
                                            <td>
                                                <span class="tm">@cell.ToString()</span>
                                            </td>
                                            }
                                    </tr>

                                    }
                                </tbody>
                            </table>

                        </div>
                    </section>
                </form>
            </div>
        </section>
    </div>
</div>
