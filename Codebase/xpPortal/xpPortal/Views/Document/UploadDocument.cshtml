<script type="text/javascript">

    var uploadedFile;
    $(document).ready(function () {
        $("#top_menu").attr("style", "display:none;");
        $('#filePANCard').on('change', function (e) {
            getFilesUploadData(e);           
        });

        $('#fileExperienceLetter').on('change', function (e) {
            getFilesUploadData(e);           
        });

        $('#fileSalarySlip').on('change', function (e) {
            getFilesUploadData(e);
        });

        $('#filedSSC').on('change', function (e) {
            getFilesUploadData(e);
        });

        $('#fileHSC').on('change', function (e) {
            getFilesUploadData(e);
        });

        $('#fileGarduation').on('change', function (e) {
            getFilesUploadData(e);
        });

        $('#filePostGraduation').on('change', function (e) {
            getFilesUploadData(e);
        });
       
    });//document ends

    function getFilesUploadData(e)
    {
        var files = e.target.files || (e.dataTransfer && e.dataTransfer.files);
        if (files.length > 0) {
            if (window.FormData !== undefined) {
                var data = new FormData();
                for (var x = 0; x < files.length; x++) {
                    data.append("file" + x, files[x]);
                }
                uploadedFile = data;
            } else {
                alert("This browser doesn't support HTML5 file uploads!");
            }
        }
    }///getfilesuploaddata

    function uploadFile()
    {
        $.ajax({
            type: "POST",
            url: '/Document/UploadHomeReport',
            contentType: false,
            processData: false,
            data: uploadedFile,
            success: function (result) {
                $("#top_menu").attr("style", "display:block;");
                if (result == "1") {
                    $("#notificationBar").html("File uploaded successufully!");                    
                }
                else { $("#notificationBar").html("File uploaded failed!"); }
            },
            error: function (xhr, status, p3, p4) {
                var err = "Error " + " " + status + " " + p3 + " " + p4;
                if (xhr.responseText && xhr.responseText[0] == "{")
                    err = JSON.parse(xhr.responseText).Message;                
            }
        });
    }


</script>
