@{
    Layout = null;
}
@Html.AntiForgeryToken()
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="ThemeBucket">
    <link rel="shortcut icon" href="images/favicon.png">
    <title>XpConnect</title>
    <!--Core CSS -->
    <link href="~/Content/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-reset.css" rel="stylesheet" />
    <link href="~/Content/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/Style.css" rel="stylesheet" />
    <link href="~/Content/style-responsive.css" rel="stylesheet" />

    <!-- Just for debugging purposes. Don't actually copy this line! -->
    <!--[if lt IE 9]>
    <script src="js/ie8-responsive-file-warning.js"></script><![endif]-->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
    <!--common script init for all pages-->
    <script src="~/Content/js/scripts.js"></script>
</head>
<body>
    <section id="container">
        <!--header start-->
        <header class="header fixed-top clearfix">
            <!--logo start-->
            <div class="brand">
                <a href="#" class="logo" style="color:#fff">
                    XpConnect
                </a>
                <div class="sidebar-toggle-box">
                    <div class="fa fa-bars"></div>
                </div>
            </div>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
                <ul class="nav top-menu container-fluid">
                    <div class="alert alert-block alert-danger fade in" style="padding: 3px 19px; margin-bottom: 0;">
                        <button data-dismiss="alert" class="close close-sm" type="button">
                            <i class="fa fa-times"></i>
                        </button>
                        <strong>Documents!</strong> Please upload your remaining documents <a href="~/Document/UploadDocument" class="btn btn-link btn-xs">Click Here</a>
                    </div>
                </ul>
                <!--  notification end -->
            </div>
            <div class="top-nav clearfix">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <!-- user login dropdown start-->
                    <li>Notifications</li>
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="~/images/avatar1_small.jpg">
                            <span class="username"> @Session["firstName"]</span>
                        </a>
                    </li>
                    <!-- user login dropdown end -->
                    <li>
                        
                        <a href="@Url.Content("~/Login/LogOff")" style="cursor: pointer; display: inline-block; font-size: 13px;padding: 8px">
                            <i class="fa fa-key"></i> Logout
                        </a>
                    </li>
                </ul>
                <!--search & user info end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse">
                <!-- sidebar menu start-->
                <div class="leftside-navigation" style="overflow-y:auto">
                    <ul class="sidebar-menu" id="nav-accordion">
                        <li>
                            <a class="active" href="~/Home/Index">
                                <i class="fa fa-dashboard"></i>
                                <span>Dashboard</span>
                            </a>
                        </li>
                        <li>
                            <a href="profile.html">
                                <i class="fa fa-user"></i>
                                <span>Profile</span>
                            </a>
                        </li>
                        @*<li>
                            <a href="profile.html">
                                <i class="fa fa-user"></i>
                                <span>Notifications</span>
                            </a>
                        </li>*@
                        <li>
                            <a href="login.html">
                                <i class="fa fa-user"></i>
                                <span>Buddy</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/Joinee/SubmitQuery">
                                <i class="fa fa-user"></i>
                                <span>Raise Query</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/Recruiter/GetQueries">
                                <i class="fa fa-user"></i>
                                <span>Queries(Tobe moved-recruiter)</span>
                            </a>
                        </li>

                        <li>
                            <a href="registration.html">
                                <i class="fa fa-user"></i>
                                <span>Relocation Assistant</span>
                            </a>
                        </li>
                        <li>
                            <a href="registration.html">
                                <i class="fa fa-user"></i>
                                <span>Refer &amp; Earn</span>
                            </a>
                        </li>
                        <li>
                            <a href="registration.html">
                                <i class="fa fa-user"></i>
                                <span>Interview Feedback</span>
                            </a>
                        </li>
                        <li>
                            <a href="registration.html">
                                <i class="fa fa-user"></i>
                                <span>Events</span>
                            </a>
                        </li>

                        <li>
                            <a href="registration.html">
                                <i class="fa fa-user"></i>
                                <span>Help</span>
                            </a>
                        </li>
                        <li>
                            <a href="registration.html">
                                <i class="fa fa-user"></i>
                                <span>Contact Us</span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                @RenderBody()
            </section>

            <!--main content end-->
        </section>
    </section>
    <!-- Placed js at the end of the document so the pages load faster -->
    <!--Core js-->
    <script src="~/Content/js/jquery.js"></script>
    <script src="~/Content/js/jquery-ui/jquery-ui-1.10.1.custom.min.js"></script>
    <script src="~/Content/bs3/js/bootstrap.min.js"></script>
    <script src="~/Content/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Content/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/js/jQuery-slimScroll-1.3.0/jquery.slimscroll.js"></script>
    <script src="~/Content/js/jquery.nicescroll.js"></script>
    <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="js/flot-chart/excanvas.min.js"></script><![endif]-->
    <script src="~/Content/js/skycons/skycons.js"></script>
    <script src="~/Content/js/jquery.scrollTo/jquery.scrollTo.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <script src="~/Content/js/calendar/clndr.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.5.2/underscore-min.js"></script>
    <script src="~/Content/js/calendar/moment-2.2.1.js"></script>
    <script src="~/Content/js/evnt.calendar.init.js"></script>
    <script src="~/Content/js/jvector-map/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Content/js/jvector-map/jquery-jvectormap-us-lcc-en.js"></script>
    <script src="~/Content/js/gauge/gauge.js"></script>
    <!--clock init-->
    <script src="~/Content/js/css3clock/js/css3clock.js"></script>
    <!--Easy Pie Chart-->
    <script src="~/Content/js/easypiechart/jquery.easypiechart.js"></script>
    <!--Sparkline Chart-->
    <script src="~/Content/js/sparkline/jquery.sparkline.js"></script>
    <!--Morris Chart-->
    <script src="~/Content/js/morris-chart/morris.js"></script>
    <script src="~/Content/js/morris-chart/raphael-min.js"></script>
    <!--jQuery Flot Chart-->
    <script src="~/Content/js/flot-chart/jquery.flot.js"></script>
    <script src="~/Content/js/flot-chart/jquery.flot.tooltip.min.js"></script>
    <script src="~/Content/js/flot-chart/jquery.flot.resize.js"></script>
    <script src="~/Content/js/flot-chart/jquery.flot.pie.resize.js"></script>
    <script src="~/Content/js/flot-chart/jquery.flot.animator.min.js"></script>
    <script src="~/Content/js/flot-chart/jquery.flot.growraf.js"></script>
    <script src="~/Content/js/dashboard.js"></script>
    <script src="~/Content/js/jquery.customSelect.min.js"></script>

    <!--script for this page-->

</body>
</html>
